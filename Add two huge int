#include<iostream>
 #include<cmath>
 #include<string>
 #include<algorithm>
 using namespace std;
 string add(string num1, string num2){
      if(num1.length() < num2.length()){
                swap(num1, num2);
                }
       int i = num1.length() - 1;
       int j = num2.length() - 1;
       int carry = 0;
       string ans = " ";
       while(i>=0 ){
        int sum;
        if(j >= 0){
            sum = num1[i]-'0'+ num2[j]-'0' + carry;
        }
        else{    
        sum = num1[i]-'0'+ carry;
        carry = sum/10;
        sum = sum%10;
        }
        ans = to_string(sum) + ans;
        i--;
        j--;
       }
         if (carry ==1){
        ans = "1" +ans;
        }
       return ans;
 }
        
 int main(){
     string num1;
     string num2;
     cout << "Can you give me a num1?"<< endl;
     cin >> num1;
     cout << "Can you give me a num2?" << endl;
     cin >> num2;
     cout <<  add ( num1, num2);
     return 0;
     }
