 #include<iostream>
 #include<cmath>
 using namespace std;
 string add(string num1, string num2){
      string sum;
      if(num1.length() > num2.length()){
                swap(num1, num2);
                }
       int i = num1.size() - 1;
       int j = num1.size() - 1;
       int carry = 0;
       string  = "";
        while(i>=0 || j>=0 || carry){
        long sum = 0;
        if(i >= 0){sum += (num1[i] - '0');i--;}
        if(j >= 0){sum += (num2[j] - '0');j--;}
        sum += carry; 
        carry = sum / 10;
        sum = sum % 10;
        return string;
        }
        }
