 #include<iostream>
 #include<cmath>
 #include<string>
 #include<algorithm>
 using namespace std;
 string add(string num1, string num2){
      string sum;
      if(num1.length() < num2.length()){
                swap(num1, num2);
                }
       int i = num1.size() - 1;
       int j = num1.size() - 1;
       int carry = 0;
       sum  = " ";
       while(i>=0 || j>=0 || carry){
        int sum = 0;
        if(i >= 0){sum += (num1[i] - '0');i--;}{
         if(j >= 0){sum += (num2[j] - '0');j--;}{
         sum += carry; 
         carry = sum / 10;
         sum = sum % 10;
         }
        }
        }
        return sum;
        }
        
 int main(){
     int num1;
     int num2;
     cin >> num1;
     cin >> num2;
     cout << string add (string num1,string num2)<< endl;
     return 0;
     }
