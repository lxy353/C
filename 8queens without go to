#include <iostream>
#include <cmath>
using namespace std;

bool ok(int q[], int c) {
     for(int i =0; i <c;i++){
         if(q[c]==q[i] || (c-i)==abs(q[c]-q[i]))
             return false;
}
     return true;
}

int backtrack(int c){
    c--;
    if(c == -1){
        exit 0;
    }
    return c;      
}

void print(int q[]) {
    static int numSolutions = 0;
    cout << "Solution #" << ++numSolutions << ": ";
    int i; 
    for(i = 0; i < 8; i++) 
    cout << q[i] << " ";
    cout << endl;
}

int main() {
    int q[8] = {0} ;   
    int c = 0;
    q[0] = 0;       
    while (c<=8) {
    q[c] ++;
    bool ok(q,c)
    }       
    while (c<=8){
    backtrack;
    print (q);
    }
    return 0;
}
