#include <iostream>
#include <cmath>
using namespace std;

bool ok(int q[8], int c) {
     for(int i =0; i <c;i++){
         ifq[c]==q[i] 
             return false;
}
     return true;
}

int backtrack(int c){
    c--;
    if(c == -1){
        exit (0);
    }
    return c;      
}

void print(int q[]) {
    static int numSolutions = 0;
    cout << "Solution #" << ++numSolutions << ": ";
    int i; 
    for(i = 0; i < 8; i++) 
    cout << q[i] << " ";
    cout << endl;
}

int main() {
    int q[8] = {0} ;   
    int c = 0;
    q[0] = 0;       
   while (c >= 0) { 
        c++;  
      if(c == 8){ 
         print(q);
        c = backtrack(c);  
    }
      while (c >= 0) {
         q[c]++;             
        if(q[c] == 8){          
          q[c] = -1;        
            c = backtrack(c);
        }
        else
            if(ok(q, c)){
            break;  
          }
    }
    }
    return 0;
}
