#include<iostream>
using namespace std;
void print(int q[]) {
   static int solution = 0;
   cout << "Solution #" << ++solution << ":\nMan\tWoman\n";
   for (int i = 0; i < 3; ++i)
      cout << i << "\t" << q[i] << "\n";
   cout << "\n";
}
int backtrack(int c){
    c--;
    if(c == -1){
        exit (0);
    }
    return c;      
}
bool ok(int c,int q[]){
   static int mp[3][3] = { { 0,2,1 },  	 
				{ 0,2,1 },	
				{ 1,2,0 } };	
   stati int wp[3][3] = { { 2,1,0 },		
				{ 0,1,2 },		
				{ 2,0,1 } };	
        

for(int i=0;i<c;i++){
   if(q[i]==q[c])
      return false;
    if(mp[c][q[i]]<mp[c][q[c]])&&(wp[q[i][c]]<wp[q[i]][i])
      return false;
return true;
}



int main() {
    int q[3] = {0} ;   
    int c = 0;
    q[0] = 0;       
   while (c >= 0) { 
        c++;  
      if(c == 3){ 
         print(q);
        c = backtrack(c);  
    }
      while (c >= 0) {
         q[c]++;             
        if(q[c] == 3){          
          q[c] = -1;        
            c = backtrack(c);
        }
        else
            if(ok(q, c)){
            break;  
          }
    }
    }
    return 0;
}
